{
  "name": "@kksushka/custom-lib-innowise",
  "version": "1.0.1",
  "description": "Custom React components library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
